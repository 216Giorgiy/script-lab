<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp
  xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0"
  xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
  <Id>af8fa5ba-4010-4bcc-9e03-a91ddadf3333</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Microsoft</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="OfficeJS Playground" />
  <Description DefaultValue="Create, test and share your Office.js snippets directly from Excel or Word." />
  <IconUrl DefaultValue="https://localhost:3000/assets/images/notepad-icon-16.png" />
  <HighResolutionIconUrl DefaultValue="https://localhost:3000/assets/images/notepad-icon-80.png" />
  <Hosts>
    <Host Name="Workbook" />
  </Hosts>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://localhost:3000" />
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
  <VersionOverrides
    xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Description resid="OfficeJsPlayground.Description" />
    <Hosts>
      <Host xsi:type="Workbook">
        <DesktopFormFactor>
          <GetStarted>
            <Title resid="OfficeJsPlayground.GetStarted.Title"/>
            <Description resid="OfficeJsPlayground.Description"/>
            <LearnMoreUrl resid="OfficeJsPlayground.GetStarted.LearnMoreUrl"/>
          </GetStarted>
          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <OfficeTab id="TabHome">
              <Group id="OfficeJsPlayground.Group">
                <Label resid="OfficeJsPlayground.GroupLabel" />
                <Icon>
                    <bt:Image size="16" resid="OfficeJsPlayground_icon_16" />
                    <bt:Image size="32" resid="OfficeJsPlayground_icon_32" />
                    <bt:Image size="80" resid="OfficeJsPlayground_icon_80" />
                </Icon>

                <Control xsi:type="Button" id="OfficeJsPlayground.TaskpaneButton">
                  <Label resid="OfficeJsPlayground.TaskpaneButton.Label" />
                  <Supertip>
                    <Title resid="OfficeJsPlayground.TaskpaneButton.TipTitle" />
                    <Description resid="OfficeJsPlayground.TaskpaneButton.TipDescription" />
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="OfficeJsPlayground_icon_16" />
                    <bt:Image size="32" resid="OfficeJsPlayground_icon_32" />
                    <bt:Image size="80" resid="OfficeJsPlayground_icon_80" />
                  </Icon>

                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>OfficeJsPlayground</TaskpaneId>
                    <SourceLocation resid="OfficeJsPlayground_Taskpane_Url" />
                  </Action>
                </Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <bt:Image id="OfficeJsPlayground_icon_16" DefaultValue="https://localhost:3000/assets/images/notepad-icon-16.png" />
        <bt:Image id="OfficeJsPlayground_icon_32" DefaultValue="https://localhost:3000/assets/images/notepad-icon-32.png" />
        <bt:Image id="OfficeJsPlayground_icon_80" DefaultValue="https://localhost:3000/assets/images/notepad-icon-80.png" />
      </bt:Images>
      <bt:Urls>
        <bt:Url id="OfficeJsPlayground_Taskpane_Url" DefaultValue="https://localhost:3000" />
        <bt:Url id="OfficeJsPlayground.GetStarted.LearnMoreUrl" DefaultValue="https://go.microsoft.com/fwlink/?LinkId=276812" />
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="OfficeJsPlayground.GroupLabel" DefaultValue="Office.js" />
        <bt:String id="OfficeJsPlayground.TaskpaneButton.Label" DefaultValue="API Playground" />
        <bt:String id="OfficeJsPlayground.TaskpaneButton.TipTitle" DefaultValue="Office.js API Playground" />
        <bt:String id="OfficeJsPlayground.GetStarted.Title" DefaultValue="Office.js API Playground" />
      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="OfficeJsPlayground.TaskpaneButton.TipDescription" DefaultValue="Office.js API Playground" />
        <bt:String id="OfficeJsPlayground.Supertip.Desc" DefaultValue="Launch the Office.js script editor." />
        <bt:String id="OfficeJsPlayground.Description" DefaultValue="Create, test and share your OfficeJS snippets directly from Word, Excel or PowerPoint." />
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>