{
  "name": "addin-playground",
  "description": "Code, run and share your Add-in snippets directly from Office.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/officedev/addin-playground"
  },
  "engines": {
    "node": ">=6.9.4"
  },
  "keywords": [
    "Office",
    "API",
    "Playground"
  ],
  "author": "Microsoft",
  "bugs": {
    "url": "https://github.com/officedev/addin-playground/issues"
  },
  "homepage": "https://github.com/officedev/addin-playground#readme",
  "version": "1.0.0",
  "scripts": {
    "clean": "rimraf dist && rimraf runner",
    "copy-runner": "cpx \"src/runner/**/!(*.ts)\" runner --watch",
    "tsc-runner": "tsc -m \"CommonJS\" -t \"ES6\" --sourceMap --outDir \"../../runner\" -w",
    "start-runner": "npm run clean && concurrently \"npm run tsc-runner \" \"npm run copy-runner\" && nodemon runner/server.js",
    "start-editor": "webpack-dev-server --config config/webpack.dev.js --progress",
    "start": "concurrently \"npm run start-editor\" \"npm run start-runner\"",
    "build": "npm run clean && webpack --config config/webpack.prod.js --colors --progress --bail"
  },
  "license": "MIT",
